# Environment Variables for DigiArogya DApp
# Copy this file to .env and update with your actual values

# Smart Contract Address (Replace with your deployed contract address)
REACT_APP_CONTRACT_ADDRESS=YOUR_DEPLOYED_CONTRACT_ADDRESS_HERE

# Network Configuration
REACT_APP_NETWORK_ID=11155111

# IPFS Configuration (if using external IPFS)
REACT_APP_IPFS_API_URL=https://ipfs.infura.io:5001
REACT_APP_IPFS_GATEWAY=https://ipfs.io/ipfs/

# Development Mode (set to false for production)
REACT_APP_DEV_MODE=true
